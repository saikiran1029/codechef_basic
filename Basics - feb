    n=int(input())
    print(math.factorial(n))

22/01/2026 - Airport Management

# cook your dish here
import sys

def solve():
    try:
        line = sys.stdin.readline()
        if not line:
            return
        t = int(line.strip())
    except ValueError:
        return

    for i in range(t):
        n = int(sys.stdin.readline().strip())
        arrivals = list(map(int, sys.stdin.readline().split()))
        departures = list(map(int, sys.stdin.readline().split()))
        time_counts = {}
        for time in arrivals:
            time_counts[time] = time_counts.get(time, 0) + 1
        for time in departures:
            time_counts[time] = time_counts.get(time, 0) + 1
        print(max(time_counts.values()))

if __name__ == "__main__":
    solve()

23/02/2026 - Broken Telephone

# cook your dish here
import sys

def solve():
    input_data = sys.stdin.read().split()
    if not input_data:
        return
    ptr = 0
    t = int(input_data[ptr])
    ptr += 1
    results = []
    for _ in range(t):
        n = int(input_data[ptr])
        ptr += 1
        a = list(map(int, input_data[ptr : ptr + n]))
        ptr += n
        is_suspect = [False] * n
        for i in range(n - 1):
            if a[i] != a[i+1]:
                is_suspect[i] = True
                is_suspect[i+1] = True
        results.append(str(sum(is_suspect)))
    sys.stdout.write("\n".join(results) + "\n")

if __name__ == "__main__":
    solve()

24/02/2026 - Chef and digits of a number

import sys

def solve():

    try:
        line = sys.stdin.readline()
        if not line:
            return
        t = int(line.strip())
    except ValueError:
        return
    for _ in range(t):
        d = sys.stdin.readline().strip()
        if not d:
            continue
        count0 = d.count('0')
        count1 = d.count('1')
        if count0 == 1 or count1 == 1:
            print("Yes")
        else:
            print("No")

if __name__ == "__main__":
    solve()

25/02/2026 - Name Reduction

import sys
from collections import Counter
def solve():
    input_data = sys.stdin.read().split()
    if not input_data:
        return
    ptr = 0
    t = int(input_data[ptr])
    ptr += 1
    results = []
    for _ in range(t):
        parent_combined = input_data[ptr] + input_data[ptr+1]
        ptr += 2
        n = int(input_data[ptr])
        ptr += 1
        children_combined = "".join(input_data[ptr : ptr + n])
        ptr += n
        parent_counts = Counter(parent_combined)
        children_counts = Counter(children_combined)
        possible = True
        for char, count in children_counts.items():
            if parent_counts[char] < count:
                possible = False
                break
        results.append("YES" if possible else "NO")
    sys.stdout.write("\n".join(results) + "\n")

if __name__ == "__main__":
    solve()

